library(HSAUR3)
library(DT)
if (pottery$kiln == 2 | pottery$kiln == 3){
pottery$region <- 2
} else if (pottery$kiln == 1){
pottery$region <- 1
} else pottery$region <- 3
datatable(pottery)
str(pottery)
table(pottery$kiln)
plot(select(pottery,Al2O3:BaO), pch=19, col=pottery$kiln)
library(tidyverse)
apply(X=select(pottery,Al2O3:BaO),2,FUN=function(x){summary(x)})
